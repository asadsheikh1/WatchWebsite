@model WatchStore.Models.categories

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<div class="row">
    <div class="col-md-3"></div>
    <div class="col-md-6">
        <div class="card card-info">
            <div class="card-header">
                <h3 class="card-title">Detail Category</h3>
            </div>
            <div class="card-body">
                <dl class="dl-horizontal">
                    <dt>
                        @Html.DisplayNameFor(model => model.categoryId)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.categoryId)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.categoryName)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.categoryName)
                    </dd>

                </dl>
                <p>
                    @Html.ActionLink("Edit", "Edit", new { id = Model.categoryId }, new { @class = "btn btn-sm btn-primary" })
                    @Html.ActionLink("Back to List", "Index", null, new { @class = "btn btn-sm btn-info" })
                </p>
            </div>

        </div>

    </div>
    <div class="col-md-3"></div>
</div>


